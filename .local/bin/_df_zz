#!/usr/bin/env bash

source ~/.scripts/helpers.sh

main() {

    if [ "$#" -eq 0 ]; then
        return 1
    fi
    
    local args=("$@")
    local _arr2
    local dia

    readarray -t _arr2 < <(cat $DF_CD_CACHE_FILE | sort -nr | awk -F '\t' '{print $2}')
    dia=$(_df_search_dir _arr2[@] args[@])
    if [ ! "$dia" == "" ]; then
        echo "$dia"
        return
    fi
    
    return 1
}

main $@